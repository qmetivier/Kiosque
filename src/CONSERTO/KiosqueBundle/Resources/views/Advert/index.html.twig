<!DOCTYPE html_5>
<html>
<head>
	<meta charset="utf-8">
	<title>Login</title>
<link rel="stylesheet" href="http://localhost/Symfony/bin/lib/bootstrap-4.0.0-alpha.6-dist/css/bootstrap.min.css">
<link rel="stylesheet" href="http://localhost/Symfony/bin/lib/font-awesome-4.7.0/css/font-awesome.min.css">
<link rel="stylesheet" href="http://localhost/Symfony/bin/css/style.css">
</head>
<body class="login">
	<div class="container">
		<div class="login_square">
			{% for message in app.session.flashbag.get('error') %}
		    <div class="row justify-content-center" style="background-color: lightgreen;">
		      {{ message }}
		    </div>
		  {% endfor %}

      	<div class="row">
					<img src="http://localhost/Symfony/bin/img/logo-conserto-blanc.png" class="img-fluid img_login">
			      {% block fos_user_content %}

				  {% endblock fos_user_content %}

          </div>
		</div>
	</div>

</body>
{% block javascripts %}
   {#Ajoutez ces lignes JavaScript si vous comptez vous servir des fonctionnalit√©s du bootstrap Twitter#}
  <script src="http://localhost/Symfony/bin/lib/tether-1.3.3/dist/js/tether.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
  <script src="http://localhost/Symfony/bin/lib/bootstrap-4.0.0-alpha.6-dist/js/bootstrap.min.js"></script>
{% endblock %}
</html>
